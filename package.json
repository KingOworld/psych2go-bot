{
  "name": "psych2go-bot",
  "version": "0.0.1",
  "description": "DiscordJS bot used in Psych2Go Discord server!",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && tsc && npm run copy-files",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 2 \"src/assets/**/*\" \"dist/assets/\"",
    "dev": "npm run prettify && npm run build && npm start",
    "prettify": "prettier -w ./src",
    "start": "node .",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psych2go-devs/psych2go-bot.git"
  },
  "author": "psych2go bot contributors",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/psych2go-devs/psych2go-bot"
  },
  "homepage": "https://github.com/psych2go-devs/psych2go-bot",
  "dependencies": {
    "copyfiles": "^2.4.1",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.7.1",
    "prettier": "^2.3.2"
  }
}
